{
  "name": "ahmed-khalil-portfolio",
  "version": "1.0.0",
  "description": "Ahmed Khalil - Senior .NET Engineer Portfolio",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:prod": "ng build --configuration production",
    "build:gh-pages": "ng build --configuration production --base-href /AhmedKhalil777/",
    "deploy": "ng build --configuration production --base-href /AhmedKhalil777/ && npx gh-pages@3.2.3 -d dist/portfolio",
    "deploy:preview": "ng build --configuration production --base-href /AhmedKhalil777/ && npx gh-pages@3.2.3 -d dist/portfolio --dotfiles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0",
    "aos": "^2.3.4",
    "particles.js": "^2.0.0",
    "@angular/material": "^18.0.0",
    "@angular/cdk": "^18.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.7.0",
    "angular-cli-ghpages": "^1.0.0",
    "gh-pages": "^3.2.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.0"
  }
}
